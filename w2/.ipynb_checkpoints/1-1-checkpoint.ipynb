   "source": [
    "class Solution:\n",
    "    def truncateSentence(self, s, k) -> str:\n",
    "        str_list = s.split()\n",
    "        final_text = ''\n",
    "        for i in range(0,k):\n",
    "            final_text += (str_list[i]+' ')\n",
    "        return final_text.strip()"
   ]
  }
