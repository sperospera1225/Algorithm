   "source": [
    "class Solution:\n",
    "    def arraySign(self, nums) -> int:\n",
    "        result = 1\n",
    "        for num in nums:\n",
    "            result *= num\n",
    "        if result > 0:\n",
    "            return 1\n",
    "        elif result == 0:\n",
    "            return 0\n",
    "        else:\n",
    "            return -1"
   ]
  }
