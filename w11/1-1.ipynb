    "class Solution:\n",
    "    def largestOddNumber(self, num) -> str:\n",
    "        for i in range(len(num)-1, -1, -1):\n",
    "            if int(num[i]) % 2 == 1 : \n",
    "                return num[0:i+1]\n",
    "        return \"\""
